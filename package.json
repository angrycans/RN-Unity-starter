{
	"name": "RNUnityStarter",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "node node_modules/react-native/local-cli/cli.js start",
		"log": "react-native log-ios",
		"test": "jest",
		"watch": "./node_modules/.bin/tsc -w",
		"compile": "./node_modules/.bin/tsc",
		"ios": "node node_modules/react-native/local-cli/cli.js run-ios --device 'Acans iphone6s' &&./node_modules/.bin/tsc -w",
		"sync": "rm -rf build/* && rsync -rv --delete --exclude=*.ts* ./src/* ./build/"
	},
	"dependencies": {
		"moment": "^2.19.2",
		"native-base": "^2.3.3",
		"plume2": "^0.2.2",
		"react": "16.0.0-beta.5",
		"react-native": "0.49.5",
		"react-native-eval": "0.1.0",
		"react-native-root-toast": "^2.2.0",
		"react-native-vector-icons-iconfont": "^1.0.6",
		"react-navigation": "^1.0.0-beta.21"
	},
	"devDependencies": {
		"@types/react": "^16.0.25",
		"@types/react-native": "^0.50.7",
		"babel-jest": "21.2.0",
		"babel-plugin-import": "^1.6.2",
		"babel-plugin-transform-rn-react-dom": "^1.0.0",
		"babel-preset-react-native": "^4.0.0",
		"jest": "21.2.1",
		"prop-types": "^15.6.0",
		"react-test-renderer": "16.0.0-beta.5",
		"tslib": "^1.8.0",
		"typescript": "^2.6.1"
	},
	"jest": {
		"preset": "react-native"
	}
}